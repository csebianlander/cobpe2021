<% include ./partials/head %>

<h1 class="ui header">Current Schedule</h1>

<div class="content">
  <table class="ui striped unstackable table">
    <thead>
      <tr><th>Team:</th>
      <th>Time and Place</th>
    </tr></thead>
    <tbody>
      <% schedule.forEach(function(assignment, team) { %>
      <tr>
        <td data-label="Team"> <%= team + 1 %>: </td>
        <td data-label="Time and Place"> <%= assignment %> </td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>

<% include ./partials/foot %>
